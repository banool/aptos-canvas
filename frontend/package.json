    "graphql": "^16.8.0",
    "prettier": "^2.8.8",
    "vite": "^4.4.9"
  },
  "homepage": "./",
  "scripts": {
    "start": "vite --port 3000",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "fmt": "prettier --write 'src/**/*.(ts|tsx)'",
    "generate-canvas-gql": "graphql-codegen --config ./src/canvas/codegen.ts && pnpm fmt",
    "generate-processor-gql": "graphql-codegen --config ./src/processor/codegen.ts && pnpm fmt"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "@typescript-eslint/no-unused-vars": "off",
      "no-throw-literal": "off",
      "no-multi-str": "off",
      "no-empty-pattern": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
